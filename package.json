    "discord.js": "^13.8.1",
    "discord.js-commando": "^0.12.3",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "prettier": "^2.2.1",
